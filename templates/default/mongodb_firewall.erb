# Generated by Chef. Changing this is useless

<% @services_list.each_pair do |service_name, port| %>
	<% @allowed_ips.each do |ip| %>
# Allow connection to <%= service_name %> for <%= ip %>	
-A FWR -s <%= ip %> -p tcp --destination-port <%= port %> -m state --state NEW,ESTABLISHED -j ACCEPT
-A OUTPUT -d <%= ip %> -p tcp --source-port <%= port %> -m state --state ESTABLISHED -j ACCEPT
	<% end -%>
<% end -%>
