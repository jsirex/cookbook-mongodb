<%= @options['logpath'] %>
{
        rotate 4
        weekly
        copytruncate
        compress
        delaycompress
        sharedscripts
        postrotate
        ##  2.0.3 or later, don't use USR1.
        #   kill -USR1 `cat /var/run/<%= @options['pidfilepath'] %>` >/dev/null 2>&1 || true
        endscript
}

